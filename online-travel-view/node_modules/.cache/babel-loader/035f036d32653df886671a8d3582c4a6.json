CAAA,KAAA,CAAA,GAAA,EAAA,CAAA,CAAA,WAAA;AACA,UAAA,OAAA,GAAA,GAAA,OAAA,CAAA,KAAA,CAAA,GAAA,EAAA,CAAA,CAAA,WAAA;AACA,SAPA,CAQA;;;AACA,cAAA,MAAA,GAAA;AACA,UAAA,OAAA,EAAA,KAAA,WADA;AAEA,UAAA,IAAA,EAAA,KAAA,QAFA;AAGA,UAAA,GAAA,EAAA,KAAA,UAHA;AAIA,UAAA,SAAA,EAAA,SAJA;AAKA,UAAA,OAAA,EAAA,OALA;AAMA,aAAA,KAAA;AANA,SAAA;AAQA,cAAA,QAAA,GAAA,MAAA,KAAA,MAAA,CAAA,IAAA,CAAA,cAAA,EAAA,MAAA,CAAA;AACA,cAAA;AAAA,UAAA;AAAA,YAAA,QAAA;AACA,aAAA,SAAA,GAAA,IAAA,CAAA,IAAA;AACA,aAAA,UAAA,GAAA,IAAA,CAAA,KAAA;AACA,OArBA,CAqBA,OAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,KAAA,CAAA,WAAA,EAAA,KAAA;AACA;AACA,KAlCA;;AAmCA,IAAA,YAAA,GAAA;AACA,WAAA,WAAA,GAAA,CAAA;AACA,WAAA,cAAA;AACA,KAtCA;;AAuCA,IAAA,iBAAA,GAAA;AACA,WAAA,UAAA,GAAA,EAAA;AACA,WAAA,YAAA;AACA,KA1CA;;AA2CA,IAAA,gBAAA,CAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,GAAA;AACA,WAAA,WAAA,GAAA,CAAA;AACA,WAAA,cAAA;AACA,KA/CA;;AAgDA,IAAA,mBAAA,CAAA,GAAA,EAAA;AACA,WAAA,WAAA,GAAA,GAAA;AACA,WAAA,cAAA;AACA;;AAnDA;AAhBA,CAAA","sourcesContent":["<template>\r\n  <el-row style=\"background-color: #FFFFFF;padding: 5px 0;border-radius: 5px;\">\r\n    <el-row style=\"padding: 10px 5px;margin: 0 5px;\">\r\n      <el-row>\r\n        <span style=\"display: inline-block;font-size: 26px;font-weight: 800;padding-left: 15px;\">酒店信息</span>\r\n        <span style=\"float: right;\">\r\n                    <el-date-picker size=\"small\" style=\"width: 220px;margin-left: 5px;\" v-model=\"searchTime\"\r\n                                    type=\"daterange\" range-separator=\"至\" start-placeholder=\"创建开始\" end-placeholder=\"创建结束\">\r\n                    </el-date-picker>\r\n                    <el-input size=\"small\" style=\"width: 188px;margin-left: 5px;margin-right: 6px;\"\r\n                              v-model=\"scenicQueryDto.name\" placeholder=\"酒店名\" clearable @clear=\"handleFilterClear\">\r\n                        <el-button slot=\"append\" @click=\"handleFilter\" icon=\"el-icon-search\"></el-button>\r\n                    </el-input>\r\n                </span>\r\n\r\n      </el-row>\r\n    </el-row>\r\n    <el-row style=\"margin: 0 15px;border-top: 1px solid rgb(245,245,245);\">\r\n      <el-row v-if=\"tableData.length === 0\">\r\n        <el-empty description=\"暂无酒店信息\"></el-empty>\r\n      </el-row>\r\n      <el-row v-else class=\"scenic-item\">\r\n        <el-col :span=\"4\" @click.native=\"hotelClick(hotel)\" v-for=\"(hotel, index) in tableData\" :key=\"index\">\r\n          <div class=\"item\">\r\n            <img :src=\"hotel.cover\" alt=\"\" srcset=\"\">\r\n            <div class=\"name\">{{ hotel.name }}</div>\r\n            <div>\r\n                            <span class=\"address\">\r\n                                <i class=\"el-icon-location\"></i>\r\n                                {{ hotel.address }}\r\n                            </span>\r\n            </div>\r\n            <div class=\"info\">\r\n              <span class=\"time\">{{ timeAgo(hotel) }}</span>\r\n              <span>联系电话&nbsp;{{ hotel.concatPhone }}</span>\r\n            </div>\r\n            <div class=\"extra-info\">\r\n              <span>{{ hotel.vendorName }}</span>\r\n            </div>\r\n          </div>\r\n        </el-col>\r\n      </el-row>\r\n      <el-pagination style=\"margin:10px 0;\" @size-change=\"handleSizeChange\" @current-change=\"handleCurrentChange\"\r\n                     :current-page=\"currentPage\" :page-sizes=\"[10, 20]\" :page-size=\"pageSize\"\r\n                     layout=\"total, sizes, prev, pager, next, jumper\" :total=\"totalItems\"></el-pagination>\r\n    </el-row>\r\n  </el-row>\r\n</template>\r\n\r\n<script>\r\nimport { timeAgo } from \"@/utils/data\"\r\nexport default {\r\n  data() {\r\n    return {\r\n      data: {},\r\n      filterText: '',\r\n      currentPage: 1,\r\n      pageSize: 10,\r\n      totalItems: 0,\r\n      tableData: [],\r\n      scenicQueryDto: {}, // 搜索条件\r\n      searchTime: [],\r\n    };\r\n  },\r\n  created() {\r\n    this.fetchFreshData();\r\n  },\r\n  methods: {\r\n    hotelClick(hotel) {\r\n      sessionStorage.setItem('hotelInfo', JSON.stringify(hotel));\r\n      // 跳转至酒店详情页\r\n      this.$router.push('/hotelDetail');\r\n    },\r\n    timeAgo(scenic) {\r\n      return timeAgo(scenic.createTime);\r\n    },\r\n    async fetchFreshData() {\r\n      try {\r\n        let startTime = null;\r\n        let endTime = null;\r\n        if (this.searchTime != null && this.searchTime.length === 2) {\r\n          const [startDate, endDate] = await Promise.all(this.searchTime.map(date => date.toISOString()));\r\n          startTime = `${startDate.split('T')[0]}T00:00:00`;\r\n          endTime = `${endDate.split('T')[0]}T23:59:59`;\r\n        }\r\n        // 请求参数\r\n        const params = {\r\n          current: this.currentPage,\r\n          size: this.pageSize,\r\n          key: this.filterText,\r\n          startTime: startTime,\r\n          endTime: endTime,\r\n          ...this.scenicQueryDto\r\n        };\r\n        const response = await this.$axios.post('/hotel/query', params);\r\n        const {data} = response;\r\n        this.tableData = data.data;\r\n        this.totalItems = data.total;\r\n      } catch (error) {\r\n        console.error('查询酒店信息异常:', error);\r\n      }\r\n    },\r\n    handleFilter() {\r\n      this.currentPage = 1;\r\n      this.fetchFreshData();\r\n    },\r\n    handleFilterClear() {\r\n      this.filterText = '';\r\n      this.handleFilter();\r\n    },\r\n    handleSizeChange(val) {\r\n      this.pageSize = val;\r\n      this.currentPage = 1;\r\n      this.fetchFreshData();\r\n    },\r\n    handleCurrentChange(val) {\r\n      this.currentPage = val;\r\n      this.fetchFreshData();\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped lang=\"scss\">\r\n.scenic-item {\r\n  .item:hover {\r\n    background-color: rgb(248, 248, 248);\r\n  }\r\n\r\n  .item {\r\n    margin-block: 20px;\r\n    padding: 30px 10px;\r\n    box-sizing: border-box;\r\n    cursor: pointer;\r\n\r\n    .extra-info {\r\n      display: flex;\r\n      justify-content: left;\r\n      align-items: center;\r\n      gap: 5px;\r\n      font-size: 12px;\r\n\r\n      span:last-child {\r\n        display: inline-block;\r\n        padding: 3px 6px;\r\n        border-radius: 3px;\r\n        background-color: aliceblue;\r\n        color: rgb(84, 49, 223);\r\n      }\r\n    }\r\n\r\n    img {\r\n      width: 100%;\r\n      min-height: 120px;\r\n      max-height: 140px;\r\n      border-radius: 5px;\r\n    }\r\n\r\n    .address {\r\n      margin-top: 10px;\r\n      display: inline-block;\r\n      width: 100px;\r\n      font-size: 12px;\r\n      overflow: hidden;\r\n      white-space: nowrap;\r\n      text-overflow: ellipsis;\r\n    }\r\n\r\n    .name {\r\n      font-size: 20px;\r\n      font-weight: 800;\r\n    }\r\n\r\n    .info {\r\n      display: flex;\r\n      justify-content: left;\r\n      gap: 5px;\r\n      margin-block: 6px;\r\n\r\n      span {\r\n        font-size: 12px;\r\n      }\r\n    }\r\n  }\r\n\r\n}\r\n</style>"],"sourceRoot":"src/views/user"}]}